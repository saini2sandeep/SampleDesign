<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sandeepsaini.multitaskapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.Base">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Spinner" />
        <activity
            android:name=".BottamNavigationActivity"
            android:label="@string/title_activity_bottam_navigation" />
        <activity
            android:name=".DrawerActivity"
            android:label="@string/title_activity_drawer"
            android:theme="@style/AppTheme.Base" />
        <activity android:name=".XyzActivity" />
        <activity android:name=".ExpendListActivity" />
        <activity android:name=".ExpendableListActivity2" />
        <activity android:name=".HomePageActivity"></activity>
    </application>

</manifest>